{
  "name": "styled-css",
  "version": "1.1.0",
  "description": "Tagged Template literal to transform CSS to inline styles",
  "repository": "https://github.com/MrLoh/styled-css",
  "author": "@MrLoh",
  "license": "MIT",
  "private": false,
  "source": "src/index.js",
  "main": "dist/index.js",
  "react-native": "dist/index.native.js",
  "scripts": {
    "bundle": "rm -rf ./dist && microbundle src/index.native.js -f cjs --target browser -o dist/index.native.js"
  },
  "dependencies": {
    "css-to-react-native": "^2.2.2"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "semi": true,
    "tabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "devDependencies": {
    "microbundle": "^0.9.0",
    "prettier": "^1.15.3"
  }
}